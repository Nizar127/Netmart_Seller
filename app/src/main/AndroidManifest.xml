<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.netmart.netmartseller">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logolocations"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logolocations"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity"/>
        <activity android:name=".ReportDetailActivity"></activity>
        <activity android:name=".Profile.BankAccActivity" />
        <activity android:name=".Profile.BizHourActivity" />
        <activity android:name=".Profile.ContactCompActivity" />
        <activity android:name=".Profile.CallSupportActivity" />
        <activity android:name=".ProfileDetailActivity" />
        <activity android:name=".Profile.EditProfileActivity" />
        <activity android:name=".OrderDetailActivity" />
        <activity android:name=".ProductDetailsActivity" />
        <activity android:name=".EditProductFormActivity" />
        <activity android:name=".EditProductActivity" />
        <activity android:name=".ViewCategoryActivity" />
        <activity android:name=".AddProductActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".MyProductActivity" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".OrderListActivity" />
        <activity android:name=".SellerOrderActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".SellerProfileActivity" />
        <activity android:name=".SellerProductManageActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>